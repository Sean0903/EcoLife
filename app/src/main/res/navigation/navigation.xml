<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.sean.green.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
<!--        <argument-->
<!--            android:name="userIDKey"-->
<!--            app:argType="string" />-->

    </fragment>

    <fragment
        android:id="@+id/shareFragment"
        android:name="com.sean.green.share.ShareFragment"
        android:label="CommunityFragment"
        tools:layout="@layout/fragment_share">


    </fragment>

    <fragment
        android:id="@+id/chartFragment"
        android:name="com.sean.green.chart.save.ChartSaveFragment"
        android:label="ChartFragment"
        tools:layout="@layout/fragment_save_chart"/>

    <fragment
        android:id="@+id/calendarFragment"
        android:name="com.sean.green.calendar.CalendarFragment"
        android:label="CalendarFragment"
        tools:layout="@layout/fragment_calendar"/>

    <fragment
        android:id="@+id/challengeFragment"
        android:name="com.sean.green.challenge.ChallengeFragment"
        android:label="ChallengeFragment"
        tools:layout="@layout/fragment_challenge"/>

    <fragment
        android:id="@+id/saveFragment"
        android:name="com.sean.green.save.SaveFragment"
        android:label="SaveFragment"
        tools:layout="@layout/fragment_save">
<!--        <argument android:name="saveKey"-->
<!--            app:argType="com.sean.green.data.Save"/>-->
    </fragment>

    <fragment
        android:id="@+id/useFragment"
        android:name="com.sean.green.use.UseFragment"
        android:label="UseFragment"
        tools:layout="@layout/fragment_use"/>

    <fragment
        android:id="@+id/dialogSaveFragment"
        android:name="com.sean.green.dialog.DialogSaveFragment"
        android:label="DialogSaveFragment"
        tools:layout="@layout/dialog_save">
    </fragment>

    <fragment
        android:id="@+id/dialogUseFragment"
        android:name="com.sean.green.dialog.DialogUseFragment"
        android:label="DialogUseFragment"
        tools:layout="@layout/dialog_use"/>

    <fragment
        android:id="@+id/eventFragment"
        android:name="com.sean.green.event.EventFragment"
        android:label="EventFragment"
        tools:layout="@layout/fragment_event">
    </fragment>

    <fragment
        android:id="@+id/dialogEvent"
        android:name="com.sean.green.dialog.DialogEventFragment"
        android:label="DialogEvent"
        tools:layout="@layout/dialog_event">
    </fragment>

    <fragment
        android:id="@+id/shareDialogFragment"
        android:name="com.sean.green.share.message.ShareDialogFragment"
        android:label="ShareDialogFragment" />

    <fragment
        android:id="@+id/dialogChallengeFragment"
        android:name="com.sean.green.dialog.DialogChallengeFragment"
        android:label="DialogChallengeFragment"
        tools:layout="@layout/dialog_challenge">
    </fragment>

    <fragment
        android:id="@+id/pagerFragment"
        android:name="com.sean.green.chart.PagerFragment"
        android:label="PagerFragment" />

