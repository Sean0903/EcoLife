        android:name="com.sean.green.chart.use.ChartUseFragment"
        android:label="ChartUseFragment"
        tools:layout="@layout/fragment_use_chart"/>

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.sean.green.login.LoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/fragment_login">
    </fragment>

    <fragment
        android:id="@+id/toShareFragment"
        android:name="com.sean.green.share.toShare.ToShareFragment"
        android:label="ToShareFragment"
        tools:layout="@layout/fragment_to_share"/>


    <action android:id="@+id/navigate_to_home_fragment" app:destination="@id/homeFragment"
        app:popUpTo="@id/navigation"
        app:popUpToInclusive="true" />
    <action android:id="@+id/navigate_to_pager_fragment" app:destination="@id/pagerFragment"
        app:popUpTo="@id/navigation"
        app:popUpToInclusive="true" />
    <action android:id="@+id/navigate_to_chart_fragment" app:destination="@id/chartFragment"
        app:popUpTo="@id/navigation"
        app:popUpToInclusive="true" />
    <action android:id="@+id/navigate_to_calendar_fragment" app:destination="@id/calendarFragment"
        app:popUpTo="@id/navigation"
        app:popUpToInclusive="true" />
    <action android:id="@+id/navigate_to_share_fragment" app:destination="@id/shareFragment"
        app:popUpTo="@id/navigation"
        app:popUpToInclusive="true" />
    <action android:id="@+id/navigate_to_event_fragment" app:destination="@id/eventFragment"
        app:popUpTo="@id/navigation"
        app:popUpToInclusive="true" />
    <action android:id="@+id/navigate_to_save_fragment" app:destination="@id/saveFragment"
        app:popUpTo="@id/navigation"
        app:popUpToInclusive="true" />
    <action android:id="@+id/navigate_to_use_fragment" app:destination="@id/useFragment"
        app:popUpTo="@id/navigation"
        app:popUpToInclusive="true" />
    <action android:id="@+id/navigate_to_challenge_fragment" app:destination="@id/challengeFragment"
        app:popUpTo="@id/navigation"
        app:popUpToInclusive="true" />
    <action android:id="@+id/navigate_to_toShare_fragment" app:destination="@id/toShareFragment"
        app:popUpTo="@id/navigation"
        app:popUpToInclusive="true" />
    <action
        android:id="@+id/action_global_loginFragment" app:destination="@id/loginFragment" />
    <action
        android:id="@+id/action_homeFragment_to_dialogSaveFragment"
        app:destination="@id/dialogSaveFragment" />
    <action
        android:id="@+id/action_homeFragment_to_dialogUseFragment"
        app:destination="@id/dialogUseFragment" />
    <action
        android:id="@+id/action_saveFragment_to_homeFragment"
        app:destination="@id/homeFragment" />
    <action
        android:id="@+id/action_eventFragment_to_dialogEvent"
        app:destination="@id/dialogEvent" />
    <action
        android:id="@+id/action_dialogShareFragment_to_shareFragment"
        app:destination="@id/shareFragment" />
    <action
        android:id="@+id/action_dialogEvent_to_eventFragment"
        app:destination="@id/eventFragment" />
    <action
        android:id="@+id/action_homeFragment_to_dialogChallengeFragment"
        app:destination="@id/dialogChallengeFragment" />
    <action
        android:id="@+id/action_dialogChallengeFragment_to_homeFragment"
        app:destination="@id/homeFragment"/>
    <action
        android:id="@+id/action_shareFragment_to_toShareFragment"
        app:destination="@id/toShareFragment" />



</navigation>