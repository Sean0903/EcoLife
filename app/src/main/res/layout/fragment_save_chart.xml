<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <data>
        <variable
            name="viewModel"
            type="com.sean.green.chart.save.ChartSaveViewModel" />
    </data>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fillViewport="true">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:visibility="visible">

            <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
                android:id="@+id/chartSwipeRefresh"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:visibility="visible">

                <TextView
                    android:id="@+id/text_underLine5"
                    android:layout_width="0dp"
                    android:layout_height="8dp"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/underline"
                    app:layout_constraintEnd_toEndOf="@+id/text_chart_page_my_save_title"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="@+id/text_chart_page_my_save_title"
                    app:layout_constraintTop_toTopOf="@+id/text_chart_page_my_save_title" />

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:layout_marginBottom="8dp"
                    android:text="2021-5-24 - 2021-5-28"
                    app:layout_constraintBottom_toTopOf="@+id/text_chart_page_my_save_title"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/text_chart_page_my_save_title"
                    style="@style/home_page_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="@string/text_chart_page_save_title"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.518"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView5" />

                <ImageView
                    android:id="@+id/image_chart_page_save_plastic"
                    android:layout_width="40dp"
                    android:layout_height="35dp"
                    android:layout_marginStart="50dp"
                    android:background="@drawable/tag_left_corners"
                    android:padding="@dimen/elevation_all"
                    android:paddingTop="@dimen/elevation_all"
                    android:scaleType="fitCenter"
                    app:layout_constraintBottom_toBottomOf="@+id/text_chart_page_save_plastic"
                    app:layout_constraintEnd_toStartOf="@+id/text_chart_page_save_plastic"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/text_chart_page_save_plastic"
                    app:srcCompat="@drawable/bag" />

                <TextView
                    android:id="@+id/text_chart_page_save_plastic"
                    android:layout_width="65dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginBottom="8dp"
                    android:background="@drawable/tag_right_corners_plastic"
                    android:hint="0"
                    android:padding="@dimen/elevation_all"
                    android:text="33"
                    android:textAlignment="center"
                    android:textColor="@color/colorWhite"
                    android:textColorHint="@color/colorWhite"
                    android:textSize="20dp"
                    app:layout_constraintBottom_toTopOf="@+id/lineChart1"
                    app:layout_constraintEnd_toStartOf="@+id/image_chart_page_save_power"
                    app:layout_constraintStart_toEndOf="@+id/image_chart_page_save_plastic"
                    app:layout_constraintTop_toBottomOf="@+id/text_chart_page_my_save_title" />

                <ImageView
                    android:id="@+id/image_chart_page_save_power"
                    android:layout_width="40dp"
                    android:layout_height="35dp"
                    android:background="@drawable/tag_left_corners"
                    android:padding="@dimen/elevation_all"
                    android:paddingTop="@dimen/elevation_all"
                    android:scaleType="fitCenter"
                    app:layout_constraintBottom_toBottomOf="@+id/text_chart_page_save_power"
                    app:layout_constraintEnd_toStartOf="@+id/text_chart_page_save_power"
                    app:layout_constraintStart_toEndOf="@+id/text_chart_page_save_plastic"
                    app:layout_constraintTop_toTopOf="@+id/text_chart_page_save_power"
                    app:srcCompat="@drawable/lighting" />

                <TextView
                    android:id="@+id/text_chart_page_save_power"
                    android:layout_width="65dp"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="16dp"
                    android:background="@drawable/tag_right_corners_power"
                    android:hint="0"
                    android:padding="@dimen/elevation_all"
                    android:text="56"
                    android:textAlignment="center"
                    android:textColor="@color/colorWhite"
                    android:textColorHint="@color/colorWhite"
                    android:textSize="20dp"
                    app:layout_constraintEnd_toStartOf="@+id/image_chart_page_save_carbon"
                    app:layout_constraintStart_toEndOf="@+id/image_chart_page_save_power"
                    app:layout_constraintTop_toTopOf="@+id/text_chart_page_save_plastic" />

                <ImageView
                    android:id="@+id/image_chart_page_save_carbon"
                    android:layout_width="40dp"
                    android:layout_height="35dp"
                    android:background="@drawable/tag_left_corners"
                    android:padding="@dimen/elevation_all"
                    android:paddingTop="@dimen/elevation_all"
                    android:scaleType="fitCenter"
                    app:layout_constraintBottom_toBottomOf="@+id/text_chart_page_save_carbon"
                    app:layout_constraintEnd_toStartOf="@+id/text_chart_page_save_carbon"
                    app:layout_constraintStart_toEndOf="@+id/text_chart_page_save_power"
                    app:layout_constraintTop_toTopOf="@+id/text_chart_page_save_carbon"
                    app:layout_constraintVertical_bias="0.0"
                    app:srcCompat="@drawable/carbon_dioxide" />

                <TextView
                    android:id="@+id/text_chart_page_save_carbon"
                    android:layout_width="65dp"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="50dp"
                    android:background="@drawable/tag_right_corners_carbon"
                    android:hint="0"
                    android:padding="@dimen/elevation_all"
                    android:text="69"
                    android:textAlignment="center"
                    android:textColor="@color/colorWhite"
                    android:textColorHint="@color/colorWhite"
                    android:textSize="20dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/image_chart_page_save_carbon"
                    app:layout_constraintTop_toTopOf="@+id/text_chart_page_save_power" />

                <com.github.mikephil.charting.charts.LineChart
                    android:id="@+id/lineChart1"
                    android:layout_width="0dp"
                    android:layout_height="300dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginEnd="4dp"
                    app:layout_constraintBottom_toTopOf="@+id/image_chart_page_calender"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/image_chart_page_save_plastic">

                </com.github.mikephil.charting.charts.LineChart>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerView_chart_page_save"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="32dp"
                    android:layout_marginEnd="16dp"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    app:layout_constraintBottom_toTopOf="@+id/button3"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/lineChart1"
                    tools:listitem="@layout/item_chart_date_save"
                    tools:visibility="visible">

                </androidx.recyclerview.widget.RecyclerView>

                <ImageView
                    android:id="@+id/image_chart_page_calender"
                    android:layout_width="32dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="60dp"
                    app:layout_constraintBottom_toTopOf="@+id/recyclerView_chart_page_save"
                    app:layout_constraintStart_toStartOf="@+id/recyclerView_chart_page_save"
                    app:layout_constraintTop_toBottomOf="@+id/lineChart1"
                    app:srcCompat="@drawable/icon_calendar" />

                <ImageView
                    android:id="@+id/image_chart_page_save_plastic_icon1"
                    android:layout_width="40dp"
                    android:layout_height="35dp"
                    android:layout_marginStart="43dp"
                    android:padding="@dimen/elevation_all"
                    android:paddingTop="@dimen/elevation_all"
                    android:scaleType="fitCenter"
                    app:layout_constraintBottom_toBottomOf="@+id/image_chart_page_calender"
                    app:layout_constraintStart_toEndOf="@+id/image_chart_page_calender"
                    app:layout_constraintTop_toTopOf="@+id/image_chart_page_calender"
                    app:srcCompat="@drawable/bag" />

                <ImageView
                    android:id="@+id/image_chart_page_save_power_icon1"
                    android:layout_width="40dp"
                    android:layout_height="35dp"
                    android:layout_marginStart="22dp"
                    android:padding="@dimen/elevation_all"
                    android:paddingTop="@dimen/elevation_all"
                    android:scaleType="fitCenter"
                    app:layout_constraintBottom_toBottomOf="@+id/image_chart_page_save_plastic_icon1"
                    app:layout_constraintStart_toEndOf="@+id/image_chart_page_save_plastic_icon1"
                    app:layout_constraintTop_toTopOf="@+id/image_chart_page_save_plastic_icon1"
                    app:srcCompat="@drawable/lighting" />

                <ImageView
                    android:id="@+id/image_chart_page_save_carbon_icon1"
                    android:layout_width="40dp"
                    android:layout_height="35dp"
                    android:layout_marginStart="14dp"
                    android:padding="@dimen/elevation_all"
                    android:paddingTop="@dimen/elevation_all"
                    android:scaleType="fitCenter"
                    app:layout_constraintBottom_toTopOf="@+id/recyclerView_chart_page_save"
                    app:layout_constraintStart_toEndOf="@+id/image_chart_page_save_power_icon1"
                    app:layout_constraintTop_toTopOf="@+id/image_chart_page_save_power_icon1"
                    app:srcCompat="@drawable/carbon_dioxide" />

                <Button
                    android:id="@+id/button3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="60dp"
                    android:text="Button"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/recyclerView_chart_page_save" />
            </androidx.constraintlayout.widget.ConstraintLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.core.widget.NestedScrollView>
</layout>